var toFixed = function (value) {
  return value.toFixed(2);
}

var subStr = function(str){
  return str.substring(0,10)
}

var parseint = function(str){
  return parseInt(str)
}

var isHas = function(str){
  return str.indexOf(',')
}

module.exports = {
  toFixed: toFixed,
  subStr: subStr,
  parseint: parseint,
  isHas : isHas
}